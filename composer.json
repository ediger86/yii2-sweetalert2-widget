{
    "name": "ediger86/yii2-sweetalert2-widget",
    "description": "Fork of Dominus77 SweetAlert widget that uses Bootstrap 5",
    "type": "yii2-extension",
    "keywords": ["dominus77","yii2","extension","widget","sweetalert2"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Schevchenko",
            "email": "ivanovosity@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-bootstrap5": "~2.0.1",
        "npm-asset/sweetalert2": "^11.1.4",
        "daneden/animate.css": "^3.5"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "6.*"
    },
    "repositories":[
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "dominus77\\sweetalert2\\": ""
        }
    }
}
